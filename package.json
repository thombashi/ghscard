{
    "name": "ghscard",
    "version": "0.0.15",
    "description": "",
    "main": "index.js",
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "ts"
        }
    },
    "scripts": {
        "build": "rimraf dist/* && tsc && webpack",
        "lint": "tslint src/*.ts",
        "package": "npm run build && uglifyjs dist/ghscard.js --compress drop_console=true -o dist/ghscard.min.js",
        "watch": "npm-watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thombashi/ghscard.git"
    },
    "keywords": [
        "github"
    ],
    "author": "Tsuyoshi Hombashi",
    "license": "MIT",
    "homepage": "https://github.com/thombashi/ghscard",
    "devDependencies": {
        "jquery": "^3.3.1",
        "moment": "^2.23.0",
        "npm-watch": "^0.5.0",
        "prettier": "^1.15.3",
        "rimraf": "^2.6.2",
        "tslint": "^5.12.0",
        "typescript": "^3.1.6",
        "uglify-js": "^3.4.4",
        "webpack": "^4.28.2",
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {}
