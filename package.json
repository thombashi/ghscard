    "moment": "^2.22.2",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.4",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {}
